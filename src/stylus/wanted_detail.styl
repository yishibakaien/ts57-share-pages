@charset 'utf-8';

.swiper-container .swiper-slide {
    position: relative;
    width: 100%;
    padding-top: 70%;
    // background: url('../../images/patterns1.png');
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
}
.swiper-container .swiper-slide:nth-of-type(2) {
    // background: url('../../images/patterns2.png');
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
}
.swiper-container .number {
    position: absolute;
    right: 4px;
    bottom: 4px;
    padding: 4px 8px;
    z-index: 3;
    color: #fff;
    background: rgba(7, 17, 27, 0.6);
    border-radius: 2px;
}
.swiper-container .number .active-number,
.swiper-container .number .total-number {
    display: inline-block;
    height: 14px;
    line-height: 14px;
}

.desc-wrapper {
    display: -webkit-flex;
    display: -webkit-box;
    display: flex;
    padding: 16px;
    background: #fff;
    /*#####最近添加#####*/
    position: relative;
}

.desc-wrapper .left {
    -webkit-flex: 1;
    flex: 1;
}
.desc-wrapper .left .supply-text{
    margin-right: 80px;
}
.desc-wrapper .left .serial-number {
    line-height: 20px;
    font-size: 16px;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    overflow: hidden;
}
.desc-wrapper .left .price {
    margin-top: 12px;
    font-size: 14px;
}
.desc-wrapper .left .price .text {
    color: #666;
}
.desc-wrapper .left .price .money {
    color: #439cfd;
}
.desc-wrapper .right {
    /*-webkit-flex: 0 0 70px;
    flex: 0 0 70px;*/
    width: 70px;
     /*#####最近添加#####*/
    position: absolute;
    right: 10px;
}
.desc-wrapper .right .save {
    border-left: 1px solid #d9d9d9;
    width: 100%;
    padding: 4px 0;
    text-align: right;
}
.desc-wrapper .right .icon-star {
    font-size: 20px;
    color: rgba(7, 17, 27, 0.3);
}
.desc-wrapper .right .text {
    font-size: 14px;
    color: #666;
}
.desc-wrapper .right .page-view {
    margin-top: 4px;
    text-align: right;
    font-size: 12px;
    color: #999;
}
.wanted-detail {
    background: #fff;
    padding-left: 16px;
}
.wanted-detail .wanted-type {
    box-sizing: border-box;
    height: 28px;
    line-height: 20px;
    font-size: 14px;
}
.wanted-detail .name {
    display: inline-block;
    text-align: right;
    color: #666;
}
.wanted-detail .wanted-status {
    display: inline-block;
    font-size: 12px;
    color: #fff;
    height: 20px;
    line-height: 20px;
    padding: 0 8px;
    text-align: center;
    background-color: #CACACA;
    border-radius: 10px;
    margin-left: 4px;
}
.wanted-detail .page-view {
    font-size: 12px;
    color: #999;
    float: right;
    margin-right: 24px;
}
.message {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: relative;
    box-sizing: border-box;
    height: 72px;
    padding: 8px 15px;
    overflow: hidden;
    background: #fff;
}
.message .avatar-wrapper {
    flex: 0 0 60px;
    -webkit-flex: 0 0 60px;
    width: 60px;
    border-radius: 5px;
    overflow: hidden;
    margin-right: 10px;
}
.message .avatar-wrapper .avatar {
    display: inline-block;
    border-radius: 5px;
}
.message .name {
    flex: 1;
}
.message .name .name-list {
    display inline-block;
    width: 100%;
    line-height: 56px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.message .content-wrapper {
    flex: 1;
    -webkit-flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: relative;
    overflow: hidden;
}
.message .content-wrapper .content {
    flex: 1;
    -webkit-flex: 1;
    position: relative;
    overflow: hidden;
    margin-top: 8px;
}
.message .content-wrapper .content .title {
    height: 16px;
    padding-right: 16px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.message .content-wrapper .content .desc {
    width: 90%;
    height: 16px;
    line-height: 16px;
    margin-top: 5px;
    font-size: 11px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #666;
}
.message .title .tag {
    margin-right: 6px;
}
.message .title .tag,.message .title .name {
    line-height: 16px;
    font-size: 14px;
    vertical-align: top;
}
.message .content-wrapper .star-wrapper {
    flex: 0 0 57px;
    -webkit-flex: 0 0 57px;
    display: flex;
    display: -webkit-flex;
    display: -webkit-box;
}
.message .icon-back{
    display: inline-block;
    position: absolute;
    height: 16px;
    line-height: 16px;
    right: 8px;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    color: #999
}
.patterns-params {
    padding: 0 16px;
    margin: 8px 0 80px 0;
    background: #fff;
}
.patterns-params .parameter {
    box-sizing: border-box;
    height: 40px;
    line-height: 40px;
    font-size: 15px;
    border-bottom: 1px dashed #d9d9d9;
}
.patterns-params .parameter:last-child {
    border-bottom: 0;
}
.patterns-params .parameter .name {
    display: inline-block;
    width: 60px;
    text-align: right;
}
.patterns-params .parameter .value {
    display: inline-block;
    margin-left: 20px;
}
/*footer*/
.footer {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    box-sizing: border-box;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 50px;
    line-height: 50px;
    text-align: center;
    background: #fff;
}
.footer .more-supply{
     -webkit-flex: 1;
    flex: 1;
    font-size: 15px;
    color: #000;
}
.footer .dress {
    -webkit-flex: 1;
    flex: 1;
    font-size: 0;
}
.footer .dress .pink {
    display: inline-block;
    font-size: 16px;
    font-weight: 700;
    color: #ff0033;
}
.footer .dress .text {
    display: inline-block;
    font-size: 15px;
}
.footer .call {
    -webkit-flex: 1;
    flex: 1;
    background: #439cfd;
    color: #fff;
}
.footer .call .icon-iconfontdianhua {
    font-size: 20px;
}
.footer .call .text {
    font-size: 15px;
}
.order-taking{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    box-sizing: border-box;
    flex-direction: column;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 40px;
    background: #fff;
}
.order-taking .order-count{
    height: 43px;
    line-height: 43px;
}
.order-taking .taking-text{
    margin-left: 9px;
}
.order-taking .taking-text .blue{
    color: #4CA1FE;
}
.order-taking .taking-text .grey{
    color: #666666;
}
.order-taking .to-order{
    -webkit-flex: 1;
    flex: 1;
    box-sizing: border-box;
    background-color: #4C93FD;
    text-align: center;
    line-height: 35px;
    color: #fff;
    height: 35px;
    border-radius: 4px;
    margin: 5px 9px;
    cursor: pointer;
}

.btn-wrapper {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    width: auto;
    padding: 16px;
}